{
    "name": "locator",
    "alternateNames": [
      "handle",
      "snap"
    ],
    "defaultNodeName": "locator",
    "path": [
      "geometry",
      "create"
    ],
    "input": {
      "locatorId": {
        "type": "flexi:string",
        "displayName": "id",
        "default": "0"
      },
      "position": {
        "type": "flexi:vector",
        "displayName": "position",
        "default": [
          0,
          0,
          0
        ]
      },
      "semanticType": {
        "type": "flexi:string",
        "displayName": "type",
        "default": ""
      }
    },
    "type": "Locator",
    "output": {
      "geometry": {
        "type": "geometry",
        "displayName": "geometry"
      },
      "points": {
        "type": "list:vector",
        "displayName": "points"
      },
      "points.x": {
        "type": "list:float",
        "displayName": "points.x"
      },
      "points.y": {
        "type": "list:float",
        "displayName": "points.y"
      },
      "points.z": {
        "type": "list:float",
        "displayName": "points.z"
      }
    }
  }