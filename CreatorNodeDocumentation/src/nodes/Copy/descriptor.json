{
    "name": "copy",
    "alternateNames": [
      "duplicate",
      "loop"
    ],
    "defaultNodeName": "copy",
    "path": [
      "geometry",
      "copy | loop"
    ],
    "input": {
      "geometry": {
        "type": "geometry",
        "displayName": "geometry"
      },
      "numCopies": {
        "type": "integer",
        "default": 1,
        "displayName": "# of copies"
      },
      "translate": {
        "type": "flexi:vector",
        "default": [
          0,
          0,
          0
        ],
        "displayName": "translate"
      },
      "rotate": {
        "type": "flexi:vector",
        "default": [
          0,
          0,
          0
        ],
        "displayName": "rotate"
      },
      "scale": {
        "type": "flexi:vector",
        "default": [
          1,
          1,
          1
        ],
        "displayName": "scale"
      },
      "uniformScale": {
        "type": "flexi:float",
        "default": 1,
        "displayName": "uniform scale"
      },
      "setIterators": {
        "type": "boolean",
        "default": false,
        "displayName": "use iterator(s)"
      },
      "iteratorTag": {
        "type": "string",
        "default": "",
        "displayName": "iterator tag"
      },
      "combineMeshes": {
        "type": "boolean",
        "default": false,
        "displayName": "combine meshes"
      }
    },
    "type": "Copy",
    "output": {
      "geometry": {
        "type": "geometry",
        "displayName": "geometry"
      },
      "points": {
        "type": "list:vector",
        "displayName": "points"
      },
      "points.x": {
        "type": "list:float",
        "displayName": "points.x"
      },
      "points.y": {
        "type": "list:float",
        "displayName": "points.y"
      },
      "points.z": {
        "type": "list:float",
        "displayName": "points.z"
      }
    },
    "nodeClass": "node"
  }